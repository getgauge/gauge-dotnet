{
  "id": "dotnet",
  "version": "0.7.5",
  "description": "C# support for gauge + .NET",
  "run": {
    "windows": [
      "launcher.cmd",
      "--start"
    ],
    "linux": [
      "launcher.sh",
      "--start"
    ],
    "darwin": [
      "launcher.sh",
      "--start"
    ]
  },
  "init": {
    "windows": [
      "launcher.cmd",
      "--init"
    ],
    "linux": [
      "launcher.sh",
      "--init"
    ],
    "darwin": [
      "launcher.sh",
      "--init"
    ]
  },
  "gaugeVersionSupport": {
    "minimum": "1.6.7",
    "maximum": ""
  },
  "lspLangId": "csharp",
  "gRPCSupport": true,
  "multithreaded": true,
  "conceptMessages": true
}
